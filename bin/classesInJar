#!/bin/bash -eu

function __classesInJar() {
  for jar in "${@--}" ;do
    unzip -qq -l ${jar} | \
      sed -r 's/[ \t]+/ /g' | cut -d' ' -f5 | egrep -E '.class$' | sed 's/.class$//' | sed 's:/:.:g' | \
        while read class ;do
          echo "${jar}:${class}"
        done
  done
}


__classesInJar "${@--}"
